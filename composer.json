{
    "name": "noouh/laravel-multiple-migrations",
    "description": "A Laravel package to run migrations from multiple paths.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Noouh For Integrated Solutions",
            "email": "info@noouh.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Noouh\\MultipleMigrations\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Noouh\\MultipleMigrations\\MultipleMigrationsServiceProvider"
            ]
        }
    }
}